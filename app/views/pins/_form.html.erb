<%= render 'shared/errors', object: @pin %>

<%= form_for @pin, html: { multipart: true } do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title, class: "form-control" %><br>
  <%= f.label :image %><br>
  <%= f.file_field :image, class: "form-control" %>
  <%= f.submit %>
<% end %>

<%= link_to "Back", pins_path %>
